#!/bin/sh
# postinst script for pohoda-realpad
set -e

. /usr/share/debconf/confmodule

for file in `ls /usr/lib/pohoda-realpad/multiflexi/*.app.json`
do
	multiflexi-cli application import-json --file $file
done
for file in `ls /usr/lib/pohoda-realpad/multiflexi/*.credential-type.json`
do
	multiflexi-json2credential-type $file
done

#DEBHELPER#
